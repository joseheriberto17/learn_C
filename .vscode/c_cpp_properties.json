{
    "configurations": [
        {
            "name": "Raspberry ARM64 Cross-Compiler",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/include",
                "/usr/lib/gcc-cross/aarch64-linux-gnu/11/include",
                "/usr/aarch64-linux-gnu/include",
                "/usr/aarch64-linux-gnu/include/c++/11",
                "/usr/aarch64-linux-gnu/include/c++/11/aarch64-linux-gnu"
            ],
            "defines": [
                "\"__aarch64__\"",
                "\"__LINUX__\"",
                "\"__ARM_ARCH=8\""
            ],
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-gcc-arm64",
            "compilerPath": "/usr/bin/aarch64-linux-gnu-gcc"
        }
    ],
    "version": 4
}